{
  "name": "",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "drizzle:update": "drizzle-kit generate:sqlite --out ./app/drizzle/migrations --schema ./app/drizzle/schema.server.ts",
    "db:migrate:local": "npm run drizzle:update && npx wrangler d1 migrations apply space-search-dev --local",
    "db:migrate": "npm run drizzle:update && npx wrangler d1 migrations apply space-search-dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.8.0",
    "@remix-run/cloudflare-pages": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.29.5",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "isbot": "^4.1.0",
    "miniflare": "^3.20231030.4",
    "postcss": "^8.4.47",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "tailwindcss": "^3.4.13",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@eslint-react/eslint-plugin": "^1.14.3",
    "@remix-run/dev": "^2.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-fetch": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.24.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
